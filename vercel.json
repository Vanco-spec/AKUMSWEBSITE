{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).html",
        "dest": "/dist/$1.html"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  